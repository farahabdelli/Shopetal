
{% extends 'base2.html.twig' %}

{% block Panier %}

    <h3> Mes commandes </h3>
<br><br>
    <table class="table table-striped table-hover ">
        <thead>
        <tr>

            <th>Total</th>
            <th>User</th>
            <th>Adressse</th>
            <th>Ville</th>
            <th>Livreur</th>
            <th>Etat</th>

        </tr>
        </thead>
        <tbody>

        {% for i in m %}
            <tr>

                <td>{{ i.total }} TND </td>
                <td>{{ i.idUser}}</td>
                <td>{{ i.adresse }}</td>
                <td>{{ i.ville }}</td>
                <td>{{ i.Livreur.nomLivreur }}</td>
                <td>{{ i.etat }}</td>


            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}