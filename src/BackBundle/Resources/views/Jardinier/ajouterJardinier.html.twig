
{% extends 'baseBack.html.twig' %}
{% block main %}

<h2>Ajouter un Jardinier</h2>
    <br><br>
<form method="POST">

        <tr>
            <td>Nom </td>
            <td><input type="text" class="form-control input-default " name="nom"></td><br/><br/>
        </tr>
        <tr>
            <td>Prenom</td>
            <td><input type="text" class="form-control input-default " name="prenom"></td><br/><br/>
        </tr>
        <tr>
            <td>Téléphone</td>
